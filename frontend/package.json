{
  "name": "book-vault",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepare": "husky",
    "prebuild": "npm run clean",
    "start": "node dist/src/main.js",
    "pre:dev": "npm run copy-resources",
    "debug": "nest start --debug --watch",
    "test:ORIG": "jest",
    "pretest": "npm run copy-resources",
    "test": "set LOG_LEVEL=info&& jest --runInBand",
    "test:istanbul": "npm t -- --coverage",
    "test:logdebug": "jest --runInBand",
    "test:dbserver": "set LOG_LEVEL=info&& set START_DB_SERVER=true&&jest --runInBand",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "asciidoctor": "node scripts/asciidoctor",
    "clean": "rimraf dist log/server.log",
    "copy-resources": "node scripts/copy-resources",
    "depcheck": "depcheck",
    "eslint": "eslint src __tests__",
    "eslint:9": "set ESLINT_USE_FLAT_CONFIG=false&& eslint src __tests__",
    "eslint:debug": "eslint --debug src __tests__",
    "invoke-webrequest": "pwsh -Command Invoke-WebRequest https://localhost:3000/rest/1 -SslProtocol Tls13 -SkipCertificateCheck",
    "leasot": "leasot --exit-nicely src/**/*.ts",
    "madge": "madge --circular src/main.ts",
    "madge:svg": "madge --image dependencies.svg src/main.ts",
    "prettier": "prettier --cache --write \"src/**/*.ts\" \"__tests__/**/*.ts\"",
    "prettier:check": "prettier --cache --check \"src/**/*.ts\" \"__tests__/**/*.ts\"",
    "readme2html": "node scripts/readme2html",
    "revealjs": "asciidoctor-revealjs .extras/doc/folien/folien.adoc",
    "snyk:auth": "echo https://app.snyk.io/account && snyk auth",
    "snyk": "snyk test",
    "sonar": "node scripts/sonar-scanner",
    "typedoc": "typedoc",
    "tsc": "tsc --pretty"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.2.4",
    "@chakra-ui/react": "3.1.1",
    "@emotion/react": "11.13.3",
    "@emotion/styled": "11.13.0",
    "@tsconfig/node22": "^22.0.0",
    "axios": "^1.7.7",
    "cypress": "13.15.2",
    "dayjs": "^1.11.13",
    "framer-motion": "11.11.13",
    "material-ui-popup-state": "^5.3.1",
    "next-themes": "0.4.3",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0",
    "prop-types": "^15.8.1",
    "react": "18.3.1",
    "react-datepicker": "7.5.0",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.2",
    "react-icons": "^5.3.0",
    "react-router-dom": "6.28.0",
    "recharts": "2.13.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.1",
    "@eslint/js": "^9.14.0",
    "@stylistic/eslint-plugin": "^2.10.1",
    "@types/cypress": "1.1.6",
    "@types/node": "^22.9.0",
    "@types/react": "18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "8.14.0",
    "@typescript-eslint/parser": "8.14.0",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "chai": "5.1.2",
    "cypress": "^13.15.2",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.13.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "eslint-plugin-regexp": "^2.6.0",
    "eslint-plugin-sonarjs": "^2.0.4",
    "eslint-plugin-unicorn": "^56.0.0",
    "globals": "^15.12.0",
    "husky": "^9.1.6",
    "path": "^0.12.7",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.14.0",
    "vite": "5.4.11",
    "vite-tsconfig-paths": "^5.1.2"
  }
}
